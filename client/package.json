{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "bootstrap": "^4.1.1",
        "classnames": "^2.2.6",
        "jwt-decode": "^2.2.0",
        "moment": "^2.22.2",
        "node-sass-chokidar": "^1.3.0",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-moment": "^0.7.9",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-scripts": "1.1.4",
        "reactstrap": "^6.2.0",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0"
    },
    "scripts": {
        "css:build": "node-sass-chokidar --source-map true --output-style compressed src/assets/css/sass/main.scss > src/assets/css/main.css",
        "css:watch": "npm run css:build && node-sass-chokidar src/assets/css/sass/main.scss > src/assets/css/main.css --watch --recursive --output-style compressed",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "proxy": "http://localhost:5000"
